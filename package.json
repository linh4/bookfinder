{
  "name": "book-finder",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://linh4.github.io/bookfinder",
  "babel": {
    "presets": [
      "react",
      "env"
    ],
    "plugins": [
      "emotion"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5",
    "react-spinners": "^0.5.1"
